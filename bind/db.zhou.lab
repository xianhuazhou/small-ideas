;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	 zhou.lab. root.zhou.lab. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	root.zhou.lab.
root.zhou.lab.	IN	A	192.168.10.2
www		IN	A	192.168.10.2
ftp		IN	A	192.168.10.2
zhou.lab.	IN	A	192.168.10.2
